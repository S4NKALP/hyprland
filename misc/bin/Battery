#!/bin/bash

export PATH="$HOME/.local/bin:$PATH"

power() { #? Power Options

    case $1 in
        reset) #? Reset Changes.
            Power_Manager "$@"
        ;;
        save) #? Save Power by disabling features
            Power_Manager "$@"
         ;;
        unset) #? Unset specific changes
            Power_Manager  "$@"
        ;;
        game) #? Toggle Game mode
            Power_Manager  "$@"
        ;;
        *)Parameters "$Function"
         ;;
    esac

}

"$@"
